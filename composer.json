{
  "name": "sahdevpalaniya/laravel-multi-sso",
  "description": "Laravel package for integrating multiple SSO providers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sahdev Palaniya",
      "email": "sahdevsinhpalaniya98@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "league/oauth2-client": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Sahdev\\SSO\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sahdev\\SSO\\SSOServiceProvider"
      ],
      "aliases": {
        "SSO": "Sahdev\\SSO\\Facades\\SSO"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}